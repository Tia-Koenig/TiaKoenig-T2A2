<h1>New Listing</h1>

<%= form_with model: Card.new do |f| %>
    <p>
        <%= f.label :title %>
        <%= f.text_field :title %>
        
    </p>
    <p>
        <%= f.label :number %>
        <%= f.number_field :number, in: 1..151 %>
    </p>
    <p>
       
    </p>
    <p>
        <%= f.label :price %>
        <%= f.number_field :price, in: 1..100000 %>
    </p>
    <p>
        <%= f.submit %>
    </p>
    
<% end %>